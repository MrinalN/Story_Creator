<header>
  <nav class="navbar navbar-expand-lg navbar-light bg-light">
    <a class="navbar-brand" href="/story">
      <img src='https://cdn.worldvectorlogo.com/logos/story.svg' width="50" height="50"> Story Creator
    </a>
    <div class="collapse navbar-collapse" id="navbarSupportedContent">
      <ul class="navbar-nav mr-auto">
        <li class="nav-item active">
          <% if(user){ %>
            <!-- Note: text changed here -->
          <a class="nav-link" href="/story/creator">Ten Word Tale</a>
          <% } else { %>
            <a class="nav-item nav-link" href="/login">Ten Word Tale</a>
        <% } %>
         <!-- Note: 'Start a Story' li added here -->
        <li class="nav-item">
          <a class="nav-link" href="#">Start a Story</a>
        </li>
        </li>
        <li class="nav-item">
          <a class="nav-link" href="#">Contribute to a Story</a>
        </li>
        </ul>
    </div>
    <% if(user){ %>
      <h5>Welcome back <%= user.name %> </h5>
      <form method="GET" action="/logout"><button class="btn btn-secondary" value='delete'>Logout</button>
      </form>
      <% } else { %>
        <p>
          <a href="/login">
            <button class="btn btn-secondary" value='delete'>Login
            </button>
          </a>
          <a href="/register">
            <button class="btn btn-secondary" value='delete'>Register
            </button>
          </a>
        </p>
      <% } %>
  </nav>
